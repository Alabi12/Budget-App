<div>
  <%= link_to root_path do %>
  <% end %>
  <h1>Add category</h1>
</div>
<div class="container">
  <%= form_for @category do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="form-group">
     <%= f.label :icon %>
      <%= f.text_field :icon %>
    </div>
    <div>
    <%= f.submit "Save" %>
    </div>
  <% end %>
</div>
